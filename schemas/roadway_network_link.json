{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "type",
    "features"
  ],
  "properties": {
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "FeatureCollection"
      ],
      "pattern": "^(.*)$"
    },
    "features": {
      "$id": "#/properties/features",
      "type": "array",
      "title": "The Features Schema",
      "items": {
        "$id": "#/properties/features/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "geometryId",
          "access",
          "area",
          "bridge",
          "highway",
          "junction",
          "key",
          "OSMlanes",
          "length",
          "maxspeed",
          "name",
          "oneway",
          "osmid",
          "ref",
          "service",
          "tunnel",
          "u",
          "v",
          "width",
          "LINK_ID",
          "LANES",
          "isTranLink",
          "isDriveLink",
          "isWalkLink",
          "isBikeLink",
          "locationReferences"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/features/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "6a22969708104ae2497244f3d079381d"
            ],
            "pattern": "^(.*)$"
          },
          "geometryId": {
            "$id": "#/properties/features/items/properties/geometryId",
            "type": "string",
            "title": "The Geometryid Schema",
            "default": "",
            "examples": [
              "4a454269d65c6619a2d7e2026295a295"
            ],
            "pattern": "^(.*)$"
          },
          "access": {
            "$id": "#/properties/features/items/properties/access",
            "type": "string",
            "title": "The Access Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "area": {
            "$id": "#/properties/features/items/properties/area",
            "type": "string",
            "title": "The Area Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "bridge": {
            "$id": "#/properties/features/items/properties/bridge",
            "type": "string",
            "title": "The Bridge Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "highway": {
            "$id": "#/properties/features/items/properties/highway",
            "type": "string",
            "title": "The Highway Schema",
            "default": "",
            "examples": [
              "residential"
            ],
            "pattern": "^(.*)$"
          },
          "junction": {
            "$id": "#/properties/features/items/properties/junction",
            "type": "string",
            "title": "The Junction Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "key": {
            "$id": "#/properties/features/items/properties/key",
            "type": "integer",
            "title": "The Key Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "OSMlanes": {
            "$id": "#/properties/features/items/properties/OSMlanes",
            "type": "integer",
            "title": "The Osmlanes Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "length": {
            "$id": "#/properties/features/items/properties/length",
            "type": "number",
            "title": "The Length Schema",
            "default": 0.0,
            "examples": [
              80.935
            ]
          },
          "maxspeed": {
            "$id": "#/properties/features/items/properties/maxspeed",
            "type": "string",
            "title": "The Maxspeed Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/features/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Arkwright Street,North Rivoli Street"
            ],
            "pattern": "^(.*)$"
          },
          "oneway": {
            "$id": "#/properties/features/items/properties/oneway",
            "type": "string",
            "title": "The Oneway Schema",
            "default": "",
            "examples": [
              "False"
            ],
            "pattern": "^(.*)$"
          },
          "osmid": {
            "$id": "#/properties/features/items/properties/osmid",
            "type": "string",
            "title": "The Osmid Schema",
            "default": "",
            "examples": [
              "18214905,18220565"
            ],
            "pattern": "^(.*)$"
          },
          "ref": {
            "$id": "#/properties/features/items/properties/ref",
            "type": "string",
            "title": "The Ref Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "service": {
            "$id": "#/properties/features/items/properties/service",
            "type": "string",
            "title": "The Service Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "tunnel": {
            "$id": "#/properties/features/items/properties/tunnel",
            "type": "string",
            "title": "The Tunnel Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "u": {
            "$id": "#/properties/features/items/properties/u",
            "type": "string",
            "title": "The U Schema",
            "default": "",
            "examples": [
              "493882040"
            ],
            "pattern": "^(.*)$"
          },
          "v": {
            "$id": "#/properties/features/items/properties/v",
            "type": "string",
            "title": "The V Schema",
            "default": "",
            "examples": [
              "187879468"
            ],
            "pattern": "^(.*)$"
          },
          "width": {
            "$id": "#/properties/features/items/properties/width",
            "type": "string",
            "title": "The Width Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "LINK_ID": {
            "$id": "#/properties/features/items/properties/LINK_ID",
            "type": "integer",
            "title": "The Link_id Schema",
            "default": 0,
            "examples": [
              224
            ]
          },
          "LANES": {
            "$id": "#/properties/features/items/properties/LANES",
            "type": "integer",
            "title": "The Lanes Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "isTranLink": {
            "$id": "#/properties/features/items/properties/isTranLink",
            "type": "integer",
            "title": "The Istranlink Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "isDriveLink": {
            "$id": "#/properties/features/items/properties/isDriveLink",
            "type": "integer",
            "title": "The Isdrivelink Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "isWalkLink": {
            "$id": "#/properties/features/items/properties/isWalkLink",
            "type": "integer",
            "title": "The Iswalklink Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "isBikeLink": {
            "$id": "#/properties/features/items/properties/isBikeLink",
            "type": "integer",
            "title": "The Isbikelink Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "locationReferences": {
            "$id": "#/properties/features/items/properties/locationReferences",
            "type": "array",
            "title": "The Locationreferences Schema",
            "items": {
              "$id": "#/properties/features/items/properties/locationReferences/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "sequence",
                "point",
                "intersectionId"
              ],
              "properties": {
                "sequence": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/sequence",
                  "type": "integer",
                  "title": "The Sequence Schema",
                  "default": 0,
                  "examples": [
                    1
                  ]
                },
                "point": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/point",
                  "type": "array",
                  "title": "The Point Schema",
                  "items": {
                    "$id": "#/properties/features/items/properties/locationReferences/items/properties/point/items",
                    "type": "number",
                    "title": "The Items Schema",
                    "default": 0.0,
                    "examples": [
                      -93.0837695,
                      44.963344
                    ]
                  }
                },
                "distanceToNextRef": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/distanceToNextRef",
                  "type": "number",
                  "title": "The Distancetonextref Schema",
                  "default": 0.0,
                  "examples": [
                    80.9850385918
                  ]
                },
                "bearing": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/bearing",
                  "type": "integer",
                  "title": "The Bearing Schema",
                  "default": 0,
                  "examples": [
                    0
                  ]
                },
                "intersectionId": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/intersectionId",
                  "type": "string",
                  "title": "The Intersectionid Schema",
                  "default": "",
                  "examples": [
                    "4d0231aa0ebb779f142c2518703ee481"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }
}
